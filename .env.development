# ============================================
# Bisheng Enterprise - Development Environment
# ============================================

ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=DEBUG

# Database
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_DB=bisheng_dev
POSTGRES_USER=bisheng_dev
POSTGRES_PASSWORD=dev_password_123
DATABASE_URL=postgresql://bisheng_dev:dev_password_123@postgres:5432/bisheng_dev

# Redis
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_URL=redis://redis:6379/0
CELERY_BROKER_URL=redis://redis:6379/1

# Milvus
MILVUS_HOST=milvus
MILVUS_PORT=19530

# Elasticsearch
ELASTICSEARCH_HOST=elasticsearch
ELASTICSEARCH_PORT=9200
ELASTICSEARCH_URL=http://elasticsearch:9200
ELASTICSEARCH_PASSWORD=

# MinIO
MINIO_HOST=minio
MINIO_ENDPOINT=http://minio:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin123
MINIO_BUCKET=bisheng-dev

# Application
SECRET_KEY=dev-secret-key-not-for-production
ADMIN_EMAIL=dev@bisheng.local
ADMIN_PASSWORD=dev123456
BISHENG_ADMIN_USER=admin@bisheng.local
BISHENG_ADMIN_PASSWORD=admin123

# API Keys (optional for dev)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# Development Features
ENABLE_DEBUG_TOOLBAR=true
ENABLE_PROFILING=true
ENABLE_HOT_RELOAD=true
RELOAD=true
WORKERS=1

# Disable SSL in development
SSL_ENABLED=false
VERIFY_SSL=false
